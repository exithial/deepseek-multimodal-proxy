{
  "name": "deepseek-multimodal-proxy",
  "version": "1.6.0",
  "description": "Proxy OpenAI-compatible que a√±ade capacidades multimodales a DeepSeek mediante Google Gemini",
  "author": "exithial <en.solis.g@gmail.com>",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "setup": "./scripts/setup.sh",
    "status": "./scripts/manage.sh status",
    "proxy:start": "./scripts/manage.sh start",
    "proxy:stop": "./scripts/manage.sh stop",
    "proxy:restart": "./scripts/manage.sh restart",
    "proxy:logs": "./scripts/manage.sh logs",
    "proxy:logs:clear": "./scripts/manage.sh logs --clear",
    "proxy:local": "./scripts/run-local.sh",
    "proxy:uninstall": "./scripts/manage.sh uninstall",
    "test:master": "node test/test-master.js",
    "test:claude": "node test/test-claude-code.js",
    "test:all": "npm run test:master && npm run test:claude",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/pdf-parse": "^1.1.5",
    "axios": "^1.6.7",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^2.4.5",
    "pdf2json": "^4.0.2",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.16",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exithial/deepseek-multimodal-proxy.git"
  },
  "bugs": {
    "url": "https://github.com/exithial/deepseek-multimodal-proxy/issues"
  },
  "homepage": "https://github.com/exithial/deepseek-multimodal-proxy#readme"
}
